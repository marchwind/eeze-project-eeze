<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/common_white_color" >
    
        
    <RelativeLayout 
        android:id="@+id/use_row"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/status_row_height"
        android:layout_marginLeft="@dimen/status_row_text_left">
       
            
            <TextView 
                android:id="@+id/login_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/status_login_date_top"
                android:textSize="@dimen/status_login_date_size"
                android:textColor="@color/setcommon_color"
                android:text="로그인 : 2014/06/12 14:12"/>
            
            <TextView android:id="@+id/login_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/login_date"
                android:textSize="@dimen/status_name_text_size"
                android:textColor="@color/setcommon_color"
                android:text="접수자 :홍길동"/>
     
        </RelativeLayout>
   
    <View 
        	android:layout_below="@id/use_row"
		    android:layout_width="fill_parent"
		    android:layout_height="1dp"
		    android:background="@color/use_line_color"/>

</RelativeLayout>
